package org.zkoss.ZKHibernate.domain;

import java.io.Serializable;
import javax.persistence.*;
import org.hibernate.annotations.Type;


@Entity
@Table(name = "mmobil")
@NamedQuery(name= "mmobil.findAll", query = "SELECT m FROM mmobil m")
public class Mmobil implements Serializable {
	private static final long serialVersionUID = 1L;
	private Integer mmobilpk;
	private String mmerekmobil;
	private String mtipemobil;
	
	public Mmobil () {
		
	}

	public Integer getMmobilpk() {
		return mmobilpk;
	}

	public void setMmobilpk(Integer mmobilpk) {
		this.mmobilpk = mmobilpk;
	}

	public String getMmerekmobil() {
		return mmerekmobil;
	}

	public void setMmerekmobil(String mmerekmobil) {
		this.mmerekmobil = mmerekmobil;
	}

	public String getMtipemobil() {
		return mtipemobil;
	}

	public void setMtipemobil(String mtipemobil) {
		this.mtipemobil = mtipemobil;
	}
	
	
	
}
