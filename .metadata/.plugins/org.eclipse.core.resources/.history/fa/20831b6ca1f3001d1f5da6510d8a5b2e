package org.zkoss.ZKHibernate;

import java.math.*;


import java.util.Iterator;
import java.util.Scanner;

public class CobaCobaAja {
	
	
	public int factorial(int n) {
		int hasil = 1;
		for (int i = 1; i < n+1; i++) {
			hasil = hasil * i;
		}
		System.out.println(hasil);
		return hasil; 
		}
	

	public static void main(String[] args) {
		
			CobaCobaAja test = new CobaCobaAja();
			test.factorial(4);
			
			Geometry geometry = new Geometry();
			System.out.println(geometry.circleArea(70));
			System.out.println(geometry.squareArea(16));
			
			
		}
	}
	

