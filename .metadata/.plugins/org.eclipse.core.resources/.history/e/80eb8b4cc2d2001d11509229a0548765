package org.zkoss.ZKHibernate;


import java.util.Iterator;
import java.util.Scanner;

public class CobaCobaAja {
	
	public int factorial(int n) {
		int hasil = 0;
		for (int i = 1; i < n; i++) {
			hasil = hasil * i;
		}
		
		return hasil; 
		}

	public static void main(String[] args) {
		
			CobaCobaAja test = new CobaCobaAja();
			test.factorial(4);
		}
	}
	

